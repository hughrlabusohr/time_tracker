<p id="notice"><%= notice %></p>

<nav class=bar>
  <li>
    <%= link_to "Manage Projects", projects_path %>
    <%= link_to "View Time Entries", time_entries_path %>
    <%= link_to "View Developers", developers_path %>
  </li>
</nav>

<h1>Listing Time Entries</h1>

<table>
  <thead>
    <tr class=tablelable>
      <th>Project ID</th>
      <th>Project Name</th>
      <th>Time Entry Date</th>
      <th>Duration</th>
      <th>Developer Name</th>
      <th>Developer ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr class=tableright>
        <td><%= time_entry.project_id %></td>
        <td><%= time_entry.project.name %></td>
        <td><%= time_entry.date %></td>
        <td><%= time_entry.duration %></td>
        <td><%= time_entry.developer.name %></td>
        <td><%= time_entry.developer_id %></td>
        <td><%= link_to 'Show', time_entry %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(time_entry) %></td>
        <td><%= link_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time entry', new_time_entry_path %>
